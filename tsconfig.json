{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "target": "es5",
    "lib": [
      "es2019",
			"dom"
    ],
    "module": "commonjs",
    "downlevelIteration": true,
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
  },
  "include": [
		"src/**/*",
		"./declarations.d.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}